  |  andi TMP3, TMP3, 1
  |   sdc1 FARG1, 8(TMP2)
  |  addiu PC, TMP3, 16+FRAME_PCALL
  |  b ->vm_call_dispatch
  |.  addiu NARGS8:RC, NARGS8:RC, -16

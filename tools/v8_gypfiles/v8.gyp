      'sources': [
        '<(V8_ROOT)/src/setup-isolate-full.cc',
        '<(generate_bytecode_builtins_list_output)',
        '<@(torque_generated_pure_headers)',
      ],
      'conditions': [
        ['want_separate_host_toolset==1', {
          'toolsets': ['host', 'target'],

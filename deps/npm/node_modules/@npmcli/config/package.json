    "tap": "^16.3.8"
  },
  "dependencies": {
    "@npmcli/map-workspaces": "^4.0.1",
    "@npmcli/package-json": "^6.0.1",
    "ci-info": "^4.0.0",
    "ini": "^5.0.0",
    "nopt": "^8.0.0",
    "proc-log": "^5.0.0",
    "semver": "^7.3.5",
    "walk-up-path": "^4.0.0"
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  },
  "templateOSS": {
    "//@npmcli/template-oss": "This file is partially managed by @npmcli/template-oss. Edits may be overwritten.",
    "version": "4.23.6",

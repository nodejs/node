    "ci-info": "^4.0.0",
    "ini": "^4.1.0",
    "nopt": "^7.0.0",
    "proc-log": "^3.0.0",
    "read-package-json-fast": "^3.0.2",
    "semver": "^7.3.5",
    "walk-up-path": "^3.0.1"
  },
  "engines": {
    "node": "^16.14.0 || >=18.0.0"
  },
  "templateOSS": {
    "//@npmcli/template-oss": "This file is partially managed by @npmcli/template-oss. Edits may be overwritten.",

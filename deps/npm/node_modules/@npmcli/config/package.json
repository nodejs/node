    "nopt": "^6.0.0",
    "proc-log": "^2.0.0",
    "read-package-json-fast": "^2.0.3",
    "semver": "^7.3.5",
    "walk-up-path": "^1.0.0"
  },
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || >=16.0.0"
  },
  "templateOSS": {

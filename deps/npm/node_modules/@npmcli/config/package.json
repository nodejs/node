    "nopt": "^5.0.0",
    "read-package-json-fast": "^2.0.3",
    "semver": "^7.3.4",
    "walk-up-path": "^1.0.0"
  },
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || >=16"

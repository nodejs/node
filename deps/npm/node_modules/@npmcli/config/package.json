{
  "name": "@npmcli/config",
  "version": "8.1.0",
  "files": [
    "bin/",
    "lib/"
  ],
  "main": "lib/index.js",
  "description": "Configuration management for the npm cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/cli.git",
    "directory": "workspaces/config"
  },
  "author": "GitHub Inc.",

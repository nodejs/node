name: irq_handler_entry
ID: 36
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;
	field:int common_padding;	offset:8;	size:4;	signed:1;

	field:int irq;	offset:12;	size:4;	signed:1;
	field:__data_loc char[] name;	offset:16;	size:4;	signed:0;
	field:void* handler;	offset:20;	size:4;	signed:0;

print fmt: "irq=%d name=%s handler=%pf", REC->irq, __get_str(name), REC->handler

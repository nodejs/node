name: kgsl_constraint
ID: 337
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;
	field:int common_padding;	offset:8;	size:4;	signed:1;

	field:__data_loc char[] device_name;	offset:12;	size:4;	signed:0;
	field:unsigned int type;	offset:16;	size:4;	signed:0;
	field:unsigned int value;	offset:20;	size:4;	signed:0;
	field:unsigned int on;	offset:24;	size:4;	signed:0;

print fmt: "d_name=%s constraint_type=%s constraint_value=%u status=%s", __get_str(device_name), __print_symbolic(REC->type, { 0, "None" }, { 1, "Pwrlevel" }), REC->value, REC->on ? "ON" : "OFF"

name: kgsl_regwrite
ID: 498
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:__data_loc char[] device_name;	offset:8;	size:4;	signed:0;
	field:unsigned int offset;	offset:12;	size:4;	signed:0;
	field:unsigned int value;	offset:16;	size:4;	signed:0;

print fmt: "d_name=%s reg=0x%x value=0x%x", __get_str(device_name), REC->offset, REC->value

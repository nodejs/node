name: ext3_allocate_blocks
ID: 207
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:dev_t dev;	offset:8;	size:4;	signed:0;
	field:ino_t ino;	offset:12;	size:4;	signed:0;
	field:unsigned long block;	offset:16;	size:4;	signed:0;
	field:unsigned long count;	offset:20;	size:4;	signed:0;
	field:unsigned long goal;	offset:24;	size:4;	signed:0;

print fmt: "dev %d,%d ino %lu count %lu block %lu goal %lu", ((unsigned int) ((REC->dev) >> 20)), ((unsigned int) ((REC->dev) & ((1U << 20) - 1))), (unsigned long) REC->ino, REC->count, REC->block, REC->goal

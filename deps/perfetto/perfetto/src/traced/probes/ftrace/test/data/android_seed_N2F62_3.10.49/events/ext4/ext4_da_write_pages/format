name: ext4_da_write_pages
ID: 298
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:dev_t dev;	offset:8;	size:4;	signed:0;
	field:ino_t ino;	offset:12;	size:4;	signed:0;
	field:__u64 b_blocknr;	offset:16;	size:8;	signed:0;
	field:__u32 b_size;	offset:24;	size:4;	signed:0;
	field:__u32 b_state;	offset:28;	size:4;	signed:0;
	field:unsigned long first_page;	offset:32;	size:4;	signed:0;
	field:int io_done;	offset:36;	size:4;	signed:1;
	field:int pages_written;	offset:40;	size:4;	signed:1;
	field:int sync_mode;	offset:44;	size:4;	signed:1;

